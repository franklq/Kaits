<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Kaits</title>
    <link rel="icon" type="image/png" sizes="16x16" href="~/assets/images/logo-icon.png">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.3.0/font/bootstrap-icons.css">
    <link href="~/assets/libs/datatables.net-bs4/css/dataTables.bootstrap4.css" rel="stylesheet">
    <link href="~/assets/dist/css/style.min.css" rel="stylesheet" asp-append-version="true">
    <link href="~/assets/extra-libs/toastr/dist/build/toastr.min.css" rel="stylesheet" asp-append-version="true">
    <link rel="stylesheet" href="~/assets/dist/css/jquery-confirm.min.css" asp-append-version="true">
    <link rel="stylesheet" href="~/assets/jquery-ui-1.12.1/jquery-ui.css" asp-append-version="true">
    <link href="https://unpkg.com/vanilla-datatables@latest/dist/vanilla-dataTables.min.css" rel="stylesheet" type="text/css">
    
    @RenderSection("headtag", required: false)
</head>
<body onload="fnValidaSesionMenu();">
    <section>

        <div id="main-wrapper">
            <header class="topbar" data-navbarbg="skin5">
                <nav class="navbar top-navbar navbar-expand-md navbar-dark">
                    <div class="navbar-header" data-logobg="skin5">
                        <a class="nav-toggler waves-effect waves-light d-block d-md-none" href="javascript:void(0)">
                            <i class="ti-menu ti-close"></i>
                        </a>
                        <a class="navbar-brand">
                            <b class="logo-icon">
                                <img src="~/assets/images/logo-icon.png" alt="homepage" class="dark-logo" />
                                <img src="~/assets/images/logo-light-icon.png" alt="homepage" class="light-logo" />
                            </b>
                            <span class="logo-text">
                                <img src="~/assets/images/logo-text.png" alt="homepage" class="dark-logo" />

                                <img src="~/assets/images/logo-light-text.png" class="light-logo" alt="homepage" />
                            </span>
                        </a>
                        <a class="topbartoggler d-block d-md-none waves-effect waves-light" href="javascript:void(0)"
                           data-toggle="collapse" data-target="#navbarSupportedContent"
                           aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                            <i class="ti-more"></i>
                        </a>
                    </div>
                    <div class="navbar-collapse collapse" id="navbarSupportedContent" data-navbarbg="skin3">
                        <ul class="navbar-nav mr-auto">
                            <li class="nav-item"> <a class="nav-link sidebartoggler d-none d-md-block waves-effect waves-dark" href="javascript:void(0)"><i class="ti-menu"></i></a> </li>
                        </ul>
                        <div class="row" id="divInactividad" hidden>
                            <h4 class="text-white text center" id="time_inactivo"> </h4>
                            &nbsp;&nbsp;&nbsp;&nbsp;
                            <h4 class="text-white text center">Inactividad&nbsp;&nbsp;&nbsp;</h4>
                        </div>
                        <ul class="navbar-nav justify-content-end">
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle waves-effect waves-dark" href="" data-toggle="dropdown"
                                   aria-haspopup="true" aria-expanded="false">
                                    <img src="../assets/images/users/Usuario1.jpg" alt="user" width="30" class="profile-pic rounded-circle" />
                                </a>
                                <div class="dropdown-menu mailbox dropdown-menu-right animated bounceInDown">
                                    <ul class="dropdown-user list-style-none">
                                        <li>
                                            <div class="dw-user-box p-3 d-flex">
                                                <div class="u-img"><img src="../assets/images/users/Usuario1.jpg" alt="user" class="rounded" width="80"></div>
                                                <div class="u-text ml-2">
                                                    <h4 class="mb-0" id="usuario-nombre"> </h4>
                                                    <p class="text-muted mb-1 font-14" id="usuario-email"></p>
                                                </div>
                                            </div>
                                        </li>
                                        <li role="separator" class="dropdown-divider"></li>
                                        <li class="user-list"><a class="px-3 py-2" asp-controller="Home" asp-action="Logout"><i class="fa fa-power-off"></i> Cerrar Sesión</a></li>
                                        <li role="separator" class="dropdown-divider"></li>
                                    </ul>
                                </div>
                            </li>
                        </ul>
                    </div>
                </nav>
            </header>
            <aside class="left-sidebar">
                <div class="scroll-sidebar">
                    <nav class="sidebar-nav">
                        <ul id="sidebarnav">
                            <li class="sidebar-item user-profile">
                                <a class="sidebar-link  waves-effect waves-dark" href="javascript:void(0)"
                                   aria-expanded="false">
                                    <img src="~/assets/images/users/Usuario1.jpg" alt="user">
                                    <span class="hide-menu" id="user-nombre"> </span>
                                </a>
                            </li>
                            <li class="nav-small-cap">
                                <i class="mdi mdi-dots-horizontal"></i>
                                <span class="hide-menu">Módulos</span>
                            </li>
                            <li class="sidebar-item">
                                <a class="sidebar-link has-arrow waves-effect waves-dark" aria-expanded="false">
                                    <i data-feather="folder" class="feather-icon"></i>
                                    <span class="hide-menu">1. Compras</span>
                                </a>
                                <ul aria-expanded="false" class="collapse first-level">
                                    <li class="sidebar-item">
                                        <a href="../Pedido/Index" class="sidebar-link">
                                            <i class="mdi mdi-adjust"></i>
                                            <span class="hide-menu"> 1.1. Generar Pedido</span>
                                        </a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </nav>
                </div>
            </aside>
            <div class="page-wrapper bg-white">
                <div class="container-fluid bg-white">
                    @RenderBody()
                </div>
            </div>
        </div>

        <div id="CargamodalLogin" class="modal" tabindex="-1" role="dialog" style="display: block; padding-right: 17px;" hidden>
            <div class="modal-dialog modal-dialog-centered">
                <div class="text-center">
                    <h4 class="text-white">Cargando...</h4>
                    <div class="spinner-grow text-light" role="status">
                        <span class="sr-only">Loading...</span>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <script src="~/assets/libs/jquery/dist/jquery.min.js"></script>
    <script src="~/assets/dist/js/jquery-1.12.4.js"></script>
    <script src="~/assets/dist/js/jquery-ui.js"></script>
    <script src="~/assets/libs/popper.js/dist/umd/popper.min.js"></script>
    <script src="~/assets/libs/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/assets/dist/js/app.min.js?0.001"></script>
    <script src="~/assets/dist/js/app.init.mini-sidebar.js?0.01"></script>
    <script src="~/assets/dist/js/app-style-switcher.js"></script>
    <script src="~/assets/libs/perfect-scrollbar/dist/perfect-scrollbar.jquery.min.js"></script>
    <script src="~/assets/extra-libs/sparkline/sparkline.js"></script>
    <script src="~/assets/dist/js/waves.js"></script>
    <script src="~/assets/dist/js/sidebarmenu.js"></script>
    <script src="~/assets/dist/js/feather.min.js"></script>
    <script src="~/assets/dist/js/custom.min.js"></script>
    <script src="~/assets/libs/datatables/media/js/jquery.dataTables.min.js?0.003"></script>
    <script src="~/assets/extra-libs/toastr/dist/build/toastr.min.js"></script>
    <script src="~/assets/dist/js/jquery-confirm.min.js"></script>
    <script type="text/javascript" src="~/js/Home/mimenu.js" asp-append-version="true"></script>
    <script src="https://unpkg.com/vanilla-datatables@latest/dist/vanilla-dataTables.min.js" type="text/javascript"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
